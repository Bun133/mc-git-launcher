{
  "name": "nuxt-electron",
  "version": "1.0.0",
  "private": true,
  "main": "entry-point.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron . --remote-debugging-port=9222",
    "build": "cross-env NODE_ENV=production nuxt build ./src/renderer && electron-builder"
  },
  "build": {
    "appId": "nuxt-electron",
    "directories": {
      "output": "build"
    }
  },
  "resolutions": {
    "@types/node": "^14.0.23"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "axios": "^0.19.2",
    "fs-extra": "^9.0.1",
    "fs-readstream-progress": "0.0.5",
    "isomorphic-git": "^1.7.3",
    "make-dir": "^3.1.0",
    "minecraft-launcher-core": "^3.14.6",
    "nuxt": "^2.13.0",
    "tar": "^6.0.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.11.2",
    "cross-env": "^7.0.2",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0"
  }
}
